{
  "name": "purescript-truffle-pet-shop",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/bower cache clean && ./node_modules/.bin/bower install",
    "start": "npm run clean && npm run cp-assets && ./node_modules/.bin/parcel index.html --no-autoinstall --open",
    "cp-assets": "mkdir -p static && cp -R static dist",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generator": "pulp build --src-path generator -m Generator --to generator.js && mkdir -p contracts/abis && node generator.js --abis contracts/abis --dest src --module Contracts && rm generator.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sectore/purescript-truffle-pet-shop.git"
  },
  "keywords": [
    "PureScript"
  ],
  "author": {
    "name": "Jens Krause",
    "email": "email@jkrause.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sectore/purescript-truffle-pet-shop/issues"
  },
  "homepage": "https://github.com/sectore/purescript-truffle-pet-shop#readme",
  "dependencies": {
    "bignumber.js": "^6.0.0",
    "ethjs-provider-http": "^0.1.6"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "crypto-js": "^3.1.9-1",
    "parcel-bundler": "^1.7.0",
    "purescript": "^0.11.7",
    "purescript-psa": "^0.6.0",
    "rimraf": "^2.6.1"
  }
}
